    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@supabase/supabase-js": "^2.2.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-google-recaptcha": "^2.1.9",
    "date-fns": "^4.1.0",
    "eslint": "^9.15.0",
